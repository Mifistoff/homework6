<%= form_for :admin, url: admin_admins_path do |f| %>
  <h1>New admin</h1>
  
  <p>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :password %>
    <%= f.text_field :password %>
  </p>

  <p>
    <%= f.label :password_confirmation %>
    <%= f.text_field :password_confirmation %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
